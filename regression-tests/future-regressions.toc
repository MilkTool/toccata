
(add-ns rt (module "regression-tester.toc"))

(defn test []
  (println "Checking future regressions")
  (let [p (future (fn []
                    (inc 9)))]
    (rt/test (= 10 (extract p))
             _FILE_ _LINE_)
    (rt/test (= 10 (extract p))
             _FILE_ _LINE_))
  (println "Futures are good"))
