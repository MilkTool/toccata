
(def vect1 (vector 1 2 3))
(def list1 (list 4 5 6))

(defn f1 [[x y z]]
  (pr* (number-str x))
  (pr* " ")
  (pr* (number-str y))
  (pr* " ")
  (pr* (number-str z))
  (pr* "\n"))

(main [_]
      (let [[x y] vect1]
        (pr* (number-str x))
        (pr* " ")
        (pr* (number-str y))
        (pr* " "))
      (let [[a b c] list1]
        (pr* (number-str a))
        (pr* " ")
        (pr* (number-str b))
        (pr* " ")
        (pr* (number-str c))
        (pr* "\n"))
      (pr* "Now for the local call sites\n")
      (let [[x y z] (vector 1 2 3)]
        (pr* (number-str x))
        (pr* " ")
        (pr* (number-str y))
        (pr* " ")
        (pr* (number-str z))
        (pr* " "))
      (let [[a b c] (list 4 5 6)]
        (pr* (number-str a))
        (pr* " ")
        (pr* (number-str b))
        (pr* " ")
        (pr* (number-str c))
        (pr* "\n"))
      (pr* "Function destructuring\n")
      (f1 vect1)
      (f1 list1)
      (pr* "Vectors and Lists! Oh my!\n"))
