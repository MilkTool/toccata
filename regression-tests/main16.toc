
(def magic "don't even ask")

(defn integer-str [n]
  (inline C String "
   return(integer_str(arg0));
"))

(defn f
  ([x]
   (pr* (integer-str x))
   (pr* "\n"))
  ([x & xs]
   (map (cons x xs) (fn [x]
                      (pr* (integer-str x))
                      (pr* " ")))
   (pr* "\n")))

(main [_]
      (f 1)
      (f 4 5 6 7))
