
(deftype Checked [m]
  (assert (instance? HashMap m))

  Associative
  (assoc [_ k v]
    (assert (instance? HashSet v))

    (or (instance? HashSet v)
        (maybe 'fail))
    (Checked (assoc m k v))))

(main [_]
  (assoc (Checked {}) "bogus" (either (maybe 'bogus)
                                      #{})))
